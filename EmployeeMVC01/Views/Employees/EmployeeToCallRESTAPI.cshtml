<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-warning">Employee To CALL REST API</h2>
    </div>
    <div class="col-3 offset-3">
        <a asp-action="UpsertToCallRESTAPI" asp-controller="Employees" class="btn btn-info form-control text-white">
            Add New Employee
        </a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Middle Name</th>
                    <th>Last Name</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/employeeToCallRESTAPI.js"></script>
}